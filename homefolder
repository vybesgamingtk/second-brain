    <script>
        // Configuration
        const CONFIG = {
            SUPABASE_URL: 'https://rrjhgbuorrhvzwmmxmzs.supabase.co',
            SUPABASE_ANON_KEY: 'eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6InJyamhnYnVvcnJodnp3bW14bXpzIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTQzMzMzNjQsImV4cCI6MjA2OTkwOTM2NH0.Fs60CPW1HKh0UEP4AIlsoq1TLV8uqvHFJhKPRUOunjk',
            RECONNECT_DELAY: 3000,
            MAX_MESSAGE_LENGTH: 500,
            MAX_ROOM_NAME_LENGTH: 20,
            MIN_ROOM_NAME_LENGTH: 3
        };

        // Initialize Supabase client
        const { createClient } = supabase;
        const supabaseClient = createClient(CONFIG.SUPABASE_URL, CONFIG.SUPABASE_ANON_KEY);

        // Utility functions for binary/base64 operations
        const CryptoUtils = {
            arrayBufferToBase64: (buffer) => {
                return btoa(String.fromCharCode(...new Uint8Array(buffer)));
            },

            base64ToArrayBuffer: (base64) => {
                return Uint8Array.from(atob(base64), c => c.charCodeAt(0)).buffer;
            },

            async exportPrivateKey(privateKey) {
                const pkcs8 = await crypto.subtle.exportKey("pkcs8", privateKey);
                return this.arrayBufferToBase64(pkcs8);
            },

            async importPrivateKey(base64) {
                const pkcs8 = this.base64ToArrayBuffer(base64);
                return crypto.subtle.importKey(
                    "pkcs8",
                    pkcs8,
                    { name: "RSA-OAEP", hash: "SHA-256" },
                    true,
                    ["decrypt"]
                );
            },

            async exportPublicKey(publicKey) {
                const spki = await crypto.subtle.exportKey("spki", publicKey);
                return this.arrayBufferToBase64(spki);
            },

            async importPublicKey(base64) {
                const spki = this.base64ToArrayBuffer(base64);
                return crypto.subtle.importKey(
                    "spki",
                    spki,
                    { name: "RSA-OAEP", hash: "SHA-256" },
                    true,
                    ["encrypt"]
                );
            }
        };

        // Authentication and UI management
        class SupabaseChatApp {
            constructor() {
                this.user = null;
                this.isSignUp = false;
                this.isLoading = false;
                this.init();
            }

            async init() {
                try {
                    // Set up auth state listener
                    supabaseClient.auth.onAuthStateChange((event, session) => {
                        if (event === 'SIGNED_IN' && session) {
                            this.user = session.user;
                            this.onSignIn();
                        } else if (event === 'SIGNED_OUT') {
                            this.onSignOut();
                        }
                    });

                    // Check existing session
                    const { data: { session } } = await supabaseClient.auth.getSession();
                    if (session?.user) {
                        this.user = session.user;
                        this.onSignIn();
                    } else {
                        this.showAuthModal();
                    }

                    this.setupEventListeners();
                } catch (error) {
                    console.error('Init error:', error);
                    this.showError('Failed to initialize app');
                }
            }

            setupEventListeners() {
                document.getElementById('authForm').addEventListener('submit', (e) => {
                    e.preventDefault();
                    this.handleAuth();
                });

                document.getElementById('switchMode').addEventListener('click', () => {
                    this.toggleAuthMode();
                });
            }

            showAuthModal() {
                document.getElementById('authModal').style.display = 'flex';
            }

            hideAuthModal() {
                document.getElementById('authModal').style.display = 'none';
            }

            toggleAuthMode() {
                this.isSignUp = !this.isSignUp;
                const title = this.isSignUp ? 'Sign Up' : 'Sign In';
                const switchText = this.isSignUp 
                    ? 'Already have an account? Sign in' 
                    : "Don't have an account? Sign up";

                document.getElementById('authTitle').textContent = title;
                document.getElementById('authButtonText').textContent = title;
                document.getElementById('switchMode').textContent = switchText;
                document.getElementById('usernameInput').style.display = this.isSignUp ? 'block' : 'none';
                
                this.clearError();
            }

            async handleAuth() {
                if (this.isLoading) return;

                const email = document.getElementById('emailInput').value.trim();
                const password = document.getElementById('passwordInput').value;
                const username = document.getElementById('usernameInput').value.trim();

                // Validation
                if (!email || !password) {
                    this.showError('Please fill in all fields');
                    return;
                }

                if (this.isSignUp && !username) {
                    this.showError('Please provide a username');
                    return;
                }

                this.setLoading(true);
                this.clearError();

                try {
                    let result;
                    if (this.isSignUp) {
                        result = await supabaseClient.auth.signUp({
                            email,
                            password,
                            options: {
                                data: { username }
                            }
                        });
                    } else {
                        result = await supabaseClient.auth.signInWithPassword({
                            email,
                            password
                        });
                    }

                    if (result.error) {
                        this.showError(result.error.message);
                        return;
                    }

                    if (this.isSignUp && !result.data.user?.confirmed_at) {
                        this.showError('Please check your email to confirm your account before signing in.');
                        return;
                    }

                    this.user = result.data.user;
                } catch (error) {
                    console.error('Auth error:', error);
                    this.showError('Authentication failed. Please try again.');
                } finally {
                    this.setLoading(false);
                }
            }

            setLoading(loading) {
                this.isLoading = loading;
                const submitBtn = document.getElementById('authSubmit');
                const buttonText = document.getElementById('authButtonText');
                const loadingSpinner = document.getElementById('authLoading');

                submitBtn.disabled = loading;
                buttonText.style.display = loading ? 'none' : 'inline';
                loadingSpinner.style.display = loading ? 'inline-block' : 'none';
            }

            showError(message) {
                const errorDiv = document.getElementById('errorMessage');
                errorDiv.textContent = message;
                errorDiv.style.display = 'block';
            }

            clearError() {
                const errorDiv = document.getElementById('errorMessage');
                errorDiv.style.display = 'none';
            }

            onSignIn() {
                this.hideAuthModal();
                document.getElementById('chatUI').style.display = 'flex';
                document.getElementById('userInfo').textContent = this.user.email;
                
                // Initialize chat
                if (window.chat) {
                    window.chat.cleanup();
                }
                window.chat = new EncryptedChat(this.user);
            }

            async signOut() {
                try {
                    if (window.chat) {
                        window.chat.cleanup();
                    }
                    await supabaseClient.auth.signOut();
                } catch (error) {
                    console.error('Sign out error:', error);
                }
            }

            onSignOut() {
                this.user = null;
                document.getElementById('chatUI').style.display = 'none';
                document.getElementById('messages').innerHTML = '';
                document.getElementById('roomInfo').style.display = 'none';
                document.getElementById('messageInput').disabled = true;
                document.getElementById('sendBtn').disabled = true;
                document.getElementById('roomInput').value = '';
                document.getElementById('messageInput').value = '';
                this.showAuthModal();
            }
        }

      // Fixed EncryptedChat class that actually uses your Supabase database
class EncryptedChat {
    constructor(user) {
        this.user = user;
        this.currentRoom = null;
        this.currentRoomId = null;
        this.encryptionKey = null;
        this.connected = false;
        this.messageChannel = null;
        this.roomChannel = null;
        
        this.init();
    }

    async init() {
        try {
            this.updateStatus('Setting up encryption...', false);
            await this.generateEncryptionKey();
            await this.ensureUserProfile();
            this.setupEventListeners();
            this.updateStatus('Ready', true);
            this.connected = true;
            this.addSystemMessage('Ready to chat! Join a room to start messaging.');
        } catch (error) {
            console.error('Chat init error:', error);
            this.updateStatus('Setup failed', false);
            this.addSystemMessage('Failed to initialize chat. Please refresh and try again.');
        }
    }

    async ensureUserProfile() {
        // Check if user profile exists in our users table
        const { data: existingUser, error } = await supabaseClient
            .from('users')
            .select('*')
            .eq('id', this.user.id)
            .single();

        if (error && error.code === 'PGRST116') {
            // User doesn't exist in our table, create profile
            const username = this.user.user_metadata?.username || 
                           this.user.email.split('@')[0];
            
            const { error: insertError } = await supabaseClient
                .from('users')
                .insert({
                    id: this.user.id,
                    username: username
                });

            if (insertError) {
                console.error('Error creating user profile:', insertError);
                throw new Error('Failed to create user profile');
            }
        } else if (error) {
            console.error('Error checking user profile:', error);
            throw new Error('Failed to verify user profile');
        }
    }

    async generateEncryptionKey() {
        this.encryptionKey = await crypto.subtle.generateKey(
            { name: 'AES-GCM', length: 256 },
            true,
            ['encrypt', 'decrypt']
        );
    }

    setupEventListeners() {
        const messageInput = document.getElementById('messageInput');
        const roomInput = document.getElementById('roomInput');

        messageInput.addEventListener('keypress', (e) => {
            if (e.key === 'Enter' && !e.shiftKey) {
                e.preventDefault();
                this.sendMessage();
            }
        });

        roomInput.addEventListener('keypress', (e) => {
            if (e.key === 'Enter') {
                e.preventDefault();
                this.joinRoom();
            }
        });
    }

    updateStatus(text, connected) {
        document.getElementById('statusText').textContent = text;
        document.getElementById('statusDot').classList.toggle('connected', connected);
    }

    async joinRoom() {
        const roomName = document.getElementById('roomInput').value.trim();
        
        if (!roomName) {
            alert('Please enter a room name');
            return;
        }

        if (roomName.length < CONFIG.MIN_ROOM_NAME_LENGTH) {
            alert(`Room name must be at least ${CONFIG.MIN_ROOM_NAME_LENGTH} characters`);
            return;
        }

        if (roomName.length > CONFIG.MAX_ROOM_NAME_LENGTH) {
            alert(`Room name must be no more than ${CONFIG.MAX_ROOM_NAME_LENGTH} characters`);
            return;
        }

        try {
            this.updateStatus('Joining room...', false);

            // Use your database function to join/create room
            const { data: roomId, error } = await supabaseClient.rpc('join_room', {
                room_name: roomName
            });

            if (error) {
                console.error('Error joining room:', error);
                alert('Failed to join room: ' + error.message);
                this.updateStatus('Ready', true);
                return;
            }

            // Cleanup previous room subscription
            if (this.messageChannel) {
                supabaseClient.removeChannel(this.messageChannel);
            }

            this.currentRoom = roomName;
            this.currentRoomId = roomId;
            
            // Enable message input
            document.getElementById('messageInput').disabled = false;
            document.getElementById('sendBtn').disabled = false;
            
            // Update UI
            document.getElementById('currentRoomName').textContent = roomName;
            document.getElementById('roomInfo').style.display = 'block';

            // Load recent messages
            await this.loadRecentMessages();

            // Subscribe to new messages
            this.subscribeToMessages();

            // Update room member count
            await this.updateRoomMemberCount();

            this.updateStatus('Connected', true);
            this.addSystemMessage(`Joined room: ${roomName}`);
            document.getElementById('messageInput').focus();

        } catch (error) {
            console.error('Join room error:', error);
            alert('Failed to join room. Please try again.');
            this.updateStatus('Ready', true);
        }
    }

    async loadRecentMessages() {
        try {
            const { data: messages, error } = await supabaseClient
                .from('messages')
                .select(`
                    id,
                    encrypted_content,
                    iv,
                    message_type,
                    created_at,
                    users!messages_sender_id_fkey(username)
                `)
                .eq('room_id', this.currentRoomId)
                .order('created_at', { ascending: true })
                .limit(50);

            if (error) {
                console.error('Error loading messages:', error);
                return;
            }

            // Clear existing messages
            document.getElementById('messages').innerHTML = '';

            // Display messages
            for (const message of messages) {
                try {
                    const decryptedText = await this.decryptMessage({
                        iv: message.iv,
                        data: message.encrypted_content
                    });
                    
                    const senderName = message.users?.username || 'Unknown';
                    const type = message.sender_id === this.user.id ? 'sent' : 'received';
                    
                    this.addMessage(senderName, decryptedText, type, message.created_at);
                } catch (decryptError) {
                    console.error('Failed to decrypt message:', decryptError);
                    // Show encrypted message indicator instead of failing
                    const senderName = message.users?.username || 'Unknown';
                    const type = message.sender_id === this.user.id ? 'sent' : 'received';
                    this.addMessage(senderName, '[Encrypted message]', type, message.created_at);
                }
            }
        } catch (error) {
            console.error('Load messages error:', error);
        }
    }

    subscribeToMessages() {
        this.messageChannel = supabaseClient
            .channel(`room-${this.currentRoomId}`)
            .on(
                'postgres_changes',
                {
                    event: 'INSERT',
                    schema: 'public',
                    table: 'messages',
                    filter: `room_id=eq.${this.currentRoomId}`
                },
                async (payload) => {
                    const message = payload.new;
                    
                    // Don't show our own messages (already displayed when sent)
                    if (message.sender_id === this.user.id) return;

                    try {
                        // Get sender username
                        const { data: sender } = await supabaseClient
                            .from('users')
                            .select('username')
                            .eq('id', message.sender_id)
                            .single();

                        const decryptedText = await this.decryptMessage({
                            iv: message.iv,
                            data: message.encrypted_content
                        });
                        
                        const senderName = sender?.username || 'Unknown';
                        this.addMessage(senderName, decryptedText, 'received', message.created_at);
                    } catch (error) {
                        console.error('Failed to process incoming message:', error);
                    }
                }
            )
            .subscribe();
    }

    async updateRoomMemberCount() {
        try {
            const { data: count, error } = await supabaseClient.rpc('get_room_member_count', {
                room_uuid: this.currentRoomId
            });

            if (!error && count !== null) {
                document.getElementById('userCount').textContent = `${count} user${count !== 1 ? 's' : ''}`;
            }
        } catch (error) {
            console.error('Error getting member count:', error);
        }
    }

    async sendMessage() {
        const messageText = document.getElementById('messageInput').value.trim();
        
        if (!messageText) return;
        if (!this.currentRoom) {
            alert('Please join a room first');
            return;
        }
        if (messageText.length > CONFIG.MAX_MESSAGE_LENGTH) {
            alert(`Message too long. Maximum ${CONFIG.MAX_MESSAGE_LENGTH} characters.`);
            return;
        }

        try {
            const encryptedData = await this.encryptMessage(messageText);
            
            // Insert message into database
            const { error } = await supabaseClient
                .from('messages')
                .insert({
                    room_id: this.currentRoomId,
                    sender_id: this.user.id,
                    encrypted_content: encryptedData.data,
                    iv: encryptedData.iv,
                    message_type: 'message'
                });

            if (error) {
                console.error('Error sending message:', error);
                alert('Failed to send message: ' + error.message);
                return;
            }

            // Add message to UI immediately
            this.addMessage('You', messageText, 'sent');
            document.getElementById('messageInput').value = '';

        } catch (error) {
            console.error('Send message error:', error);
            alert('Failed to send message. Please try again.');
        }
    }

    async encryptMessage(text) {
        const iv = crypto.getRandomValues(new Uint8Array(12));
        const encodedText = new TextEncoder().encode(text);
        
        const encrypted = await crypto.subtle.encrypt(
            { name: 'AES-GCM', iv },
            this.encryptionKey,
            encodedText
        );

        return {
            iv: btoa(String.fromCharCode(...iv)),
            data: btoa(String.fromCharCode(...new Uint8Array(encrypted)))
        };
    }

    async decryptMessage(encryptedData) {
        try {
            const iv = new Uint8Array(atob(encryptedData.iv).split('').map(c => c.charCodeAt(0)));
            const data = new Uint8Array(atob(encryptedData.data).split('').map(c => c.charCodeAt(0)));
            
            const decrypted = await crypto.subtle.decrypt(
                { name: 'AES-GCM', iv },
                this.encryptionKey,
                data
            );

            return new TextDecoder().decode(decrypted);
        } catch (error) {
            console.error('Decrypt error:', error);
            return '[Encrypted message - decryption failed]';
        }
    }

    addMessage(sender, text, type, timestamp = null) {
        const messagesContainer = document.getElementById('messages');
        const messageDiv = document.createElement('div');
        messageDiv.className = `message ${type}`;
        
        const contentDiv = document.createElement('div');
        contentDiv.className = 'message-content';
        contentDiv.textContent = text;
        
        const metaDiv = document.createElement('div');
        metaDiv.className = 'message-meta';
        
        const time = timestamp ? new Date(timestamp).toLocaleTimeString() : new Date().toLocaleTimeString();
        metaDiv.textContent = `${sender} ‚Ä¢ ${time}`;
        
        messageDiv.appendChild(contentDiv);
        messageDiv.appendChild(metaDiv);
        messagesContainer.appendChild(messageDiv);
        
        // Auto-scroll to bottom
        messagesContainer.scrollTop = messagesContainer.scrollHeight;
    }

    addSystemMessage(text) {
        this.addMessage('System', text, 'system');
    }

    cleanup() {
        // Clean up subscriptions
        if (this.messageChannel) {
            supabaseClient.removeChannel(this.messageChannel);
            this.messageChannel = null;
        }
        
        if (this.roomChannel) {
            supabaseClient.removeChannel(this.roomChannel);
            this.roomChannel = null;
        }
    }
}

        // Initialize the application
        let chatApp;
        let chat;

        document.addEventListener('DOMContentLoaded', () => {
            chatApp = new SupabaseChatApp();
        });
    </script>
</body>
</html>
